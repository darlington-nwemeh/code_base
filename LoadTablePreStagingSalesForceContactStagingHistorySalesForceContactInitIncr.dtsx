                }
                finally
                {
                    if (vars != null)
                    {
                        vars.Unlock();
                        isUnlocked = true;
                    }
                }
            }
            catch (Exception)
            {
                Dts.VariableDispenser.Reset();

                throw;
            }
            finally
            {
